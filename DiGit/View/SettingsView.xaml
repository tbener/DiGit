<mui:ModernWindow
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                  xmlns:mui="http://firstfloorsoftware.com/ModernUI"
                  xmlns:hk="clr-namespace:BondTech.HotKeyManagement.WPF._4;assembly=HotKeyManagement.WPF.4" x:Class="DiGit.View.SettingsView"
                  mc:Ignorable="d" 
                  d:DesignHeight="300" d:DesignWidth="300"
                  Title="DiGit Settings"
                  Style="{StaticResource BlankWindow}">
    <StackPanel Orientation="Vertical">
        
        <TextBox Text="{Binding P}" />
        <TextBox  Text="{Binding L}" Width="70"/>

        <Button Command="{Binding GoCommand}" Width="70">GO</Button>
        <TextBlock Text="{Binding results}" Height="36" Margin="30" />

    </StackPanel>
</mui:ModernWindow>
